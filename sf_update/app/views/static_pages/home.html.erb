
<h1>Update User</h1>

<%= form_for(@sf_margin_report) do |f| %>
	  <%= f.label :user_id %>
      <%= f.text_field :user_id %>
      <br>
      <br>
      <%= f.label :margin_report_url %>
      <%= f.text_field :margin_report_url %>

      <%= f.hidden_field :id %>
      <br>
      <br>
      <%= f.submit "Update Account", class: "btn btn-primary" %>
    <% end %>
